            <Style Selector="^.LightGreen">
                <Setter Property="BorderBrush" Value="{DynamicResource LabelTagGhostLightGreenBorderBrush}" />
                <Setter Property="Foreground" Value="{DynamicResource LabelTagGhostLightGreenForeground}" />
